<div class="modal-box">
    <h3 class="font-bold text-lg">Delete Connection: <b>
            <%= model.CONN_ID %>
    </h3>


    <form action="/connectionDelete/<%= model.CONN_ID %>" method="post">
        <input type="sourceApp" id="sourceApp" class="form-control mb-4" name="sourceApp"
            placeholder="Source Application" value="<%= model.SOURCE_ASSET_NAME %>" disabled>
        <input type="destApp" id="destApp" class="form-control mb-4" name="destApp"
            placeholder="Destination Application" value="<%= model.DEST_ASSET_NAME %>" disabled>
        <input type="proto" id="proto" class="form-control mb-4" name="proto" placeholder="Protocol Used"
            value="<%= model.PROTOCOL %>" disabled>
        <input type="enc" id="enc" class="form-control mb-4" name="enc" placeholder="Encryption Type"
            value="<%= model.ENCRYPTION %>" disabled>
        <input type="serverAuth" id="serverAuth" class="form-control mb-4" name="serverAuth"
            placeholder="Server Authentication" value="<%= model.SERVER_AUTHENTICATION %>" disabled>
        <input type="clientAuth" id="clientAuth" class="form-control mb-4" name="clientAuth"
            placeholder="Client Authentication" value="<%= model.CLIENT_AUTHENTICATION %>" disabled>
        <input type="clientAuthz" id="clientAuthz" class="form-control mb-4" name="clientAuthz"
            placeholder="Client Authorisation" value="<%= model.CLIENT_AUTHORISATION %>" disabled>
        <input type="serverCRL" id="serverCRL" class="form-control mb-4" name="serverCRL" placeholder="Server CRL"
            value="<%= model.SERVER_CRL %>" disabled>
        <input type="clientCRL" id="clientCRL" class="form-control mb-4" name="clientCRL" placeholder="Client CRL"
            value="<%= model.CLIENT_CRL %>" disabled>
        <input type="desc" id="desc" class="form-control mb-4" name="desc" placeholder="Description"
            value="<%= model.DESCRIPTION %>" disabled>

        <div class="modal-action">
            <div class="absolute bottom-0 left-2 mb-2"><label for="my-dataModal"
                    class="btn btn-warning btn-sm">Cancel</label>
            </div>
            <div class="absolute bottom-0 right-2 mb-2 "><button id="subm" type="submit"
                    class="btn btn-warning btn-sm">Delete</button></div>
        </div>
    </form>


</div>
