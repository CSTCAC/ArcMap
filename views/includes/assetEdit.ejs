<div class="modal-box">
    <h3 class="font-bold text-lg">Edit Asset: <b>
            <%= model.ASSET_ID %>
    </h3>

    <form action="/assetEdit/<%= model.ASSET_ID %>" method="post">
        <input type="assetName" name="assetName" id="assetName" class="form-control mb-4" placeholder="Name of Asset"
            value="<%= model.ASSET_NAME %>">
        <input type="zoneName" name="zoneName" id="zoneName" class="form-control mb-4" placeholder="Name of Zone"
            value="<%= model.ASSET_ZONE %>">
        <textarea type="description" name="description" id="description" class="form-control mb-4"
            placeholder="Description of Asset"><%= model.DESCRIPTION %></textarea>

        <div class="modal-action">
            <div class="absolute bottom-0 left-2 mb-2"><label for="my-dataModal"
                    class="btn btn-warning btn-sm">Cancel</label>
            </div>
            <div class="absolute bottom-0 right-2 mb-2 "><button id="subm" type="submit"
                    class="btn btn-warning btn-sm">Edit</button></div>
        </div>
    </form>
</div>